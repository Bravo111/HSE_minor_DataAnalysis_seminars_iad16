i = 1
j = 0
x_min, x_max = -10, 200
y_min, y_max = -10, 200
plot_step = 1
xx, yy = np.meshgrid(np.arange(x_min, x_max, plot_step),
                     np.arange(y_min, y_max, plot_step))
clf.fit(X[:, [i, j]], y)

z = clf.predict(np.c_[xx.ravel(), yy.ravel()])
z = z.reshape(xx.shape)
cs = plt.contourf(xx, yy, z, cmap=plt.cm.Paired) # picture
plt.axis("tight")

colors = cm.rainbow(np.linspace(0, 1, 5))
for c in range(1, 6):
    plt.scatter(X[y==c, i], X[y==c, j], c=colors[c-1])

